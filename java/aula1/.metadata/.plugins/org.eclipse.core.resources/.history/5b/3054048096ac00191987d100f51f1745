package com.db1.db1start;

public class TesteSubstring {

	public static void main(String[] args) {
		
		String texto = "AleMar";
		int quantLetras = texto.length();
		
		String partTexto = texto.substring(2, quantLetras);
		
		System.out.println(texto + " " + quantLetras + " " + partTexto);
		
	}
	
}
